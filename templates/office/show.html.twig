{% extends 'base.html.twig' %}

{% block title %}Office{% endblock %}

{% block body %}
    <h1>Office</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ office.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ office.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ office.code }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ office.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ office.createdAt ? office.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ office.updatedAt ? office.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_office_index') }}">back to list</a>

    <a href="{{ path('app_office_edit', {'id': office.id}) }}">edit</a>

    {{ include('office/_delete_form.html.twig') }}
{% endblock %}
